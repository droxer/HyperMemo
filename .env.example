# Browser / Vite environment variables
VITE_SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
# Optional override if you route Edge Functions through a custom domain.
# Otherwise leave unset and the app derives https://<project-ref>.functions.supabase.co
VITE_SUPABASE_FUNCTION_URL=https://YOUR_PROJECT_REF.functions.supabase.co
VITE_SUPABASE_ANON_KEY=replace-with-your-anon-key
VITE_GOOGLE_OAUTH_CLIENT_ID=replace-with-google-oauth-client-id
VITE_AUTO_ANON_LOGIN=false
VITE_AUTH_WAIT_TIMEOUT_MS=5000
